{
  "name": "Denis Replacer",
  "version": "1.0",
  "description": "Replaces all images on a given page with pictures of DENIS. AH!",
  "icons": { 
             "48"  : "img/icon_48.png",
			 "128" : "img/icon_128.png"
             },
  "browser_action": {
    "default_icon": "img/icon_48.png",
    "default_title": "Denis Replacer"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts" : [
   {
     "js": ["denisReplacer.js"],
     "matches": ["*://*/*"]
   }
],

  "manifest_version": 2
}
